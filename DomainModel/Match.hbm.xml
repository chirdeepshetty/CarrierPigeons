<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="CarrierPigeons" namespace="DomainModel">
  <class name="DomainModel.Match" table="Match">
    <id column="Id" name="Id">
      <generator class="native" />
    </id>
    <property name="Status" column="Status" />
    <many-to-one class="DomainModel.Request" column="request_id" name="Request" cascade="all" />
    <many-to-one class="DomainModel.Journey" column="journey_id" name="Journey" cascade="all" />
  </class>
</hibernate-mapping>