<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="DomainModel" namespace="DomainModel">
  <class name="DomainModel.Location" table="Location">
    <id name="Id">
      <generator class="native" />
    </id>
    <property name="Place" column="Place" type="String" length="50"/>
    <property name="Name.LastName" column="LastName" type="String" length="40"/>
    <property name="Email.EmailAddress" column="Email" type="String" length="60"/>
  </class>
</hibernate-mapping>