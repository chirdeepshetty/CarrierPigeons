<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="orm_intro" namespace="orm_intro">
  <class name="DomainModel.User" table="User">
    <id name="Id">
      <generator class="native" />
    </id>
    <property name="Name.FirstName" column="FirstName" type="String" length="40"/>
    <property name="Name.LastName" column="LastName" type="String" length="40"/>
    <property name="Email.EmailAddress" column="Email" type="String" length="60"/>
  </class>
</hibernate-mapping>